procedure TovF_Principal.Autor1Click(Sender: TObject);
begin
  ShowMessage('Meu nome é Adriel Matheus Graupp, tenho 20 anos, moro na cidade de Guarujá do Sul em SC, cursei o EMIEP (Ensino Medio Integrado a Educação Profissional Técnico em Informática), atualmente curso Ciência da Computação pela UNOESC de São Miguel do Oeste.');
end;

procedure TovF_Principal.CadastrarEmprestimos1Click(Sender: TObject);
begin
  ovF_EM005 := TovF_EM005.Create( application );
